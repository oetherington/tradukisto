{
	"name": "tradukisto",
	"version": "0.0.2",
	"description": "Generate Typescript types for SQL queries",
	"homepage": "https://github.com/oetherington/tradukisto#readme",
	"bugs": {
		"url": "https://github.com/oetherington/tradukisto/issues"
	},
	"repository": "github:oetherington/tradukisto",
	"license": "AGPL-3.0-or-later",
	"author": {
		"name": "Ollie Etherington",
		"url": "https://www.etherington.xyz"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": "dist/Cli.js",
	"files": [
		"/dist"
	],
	"scripts": {
		"build": "tsup",
		"check-format": "prettier --check .",
		"format": "prettier . --write --log-level warn",
		"lint": "eslint . --ignore-pattern=dist/*",
		"prepare": "husky",
		"test": "vitest run --coverage",
		"typecheck": "tsc --noEmit --skipLibCheck"
	},
	"dependencies": {
		"glob": "^11.0.3",
		"node-sql-parser": "^5.3.10",
		"postgres": "^3.4.7"
	},
	"devDependencies": {
		"@electric-sql/pglite": "^0.3.5",
		"@eslint/js": "^9.32.0",
		"@types/node": "^24.1.0",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.32.0",
		"husky": "^9.1.7",
		"prettier": "^3.6.2",
		"prettier-plugin-packagejson": "^2.5.19",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vitest": "^3.2.4"
	}
}
